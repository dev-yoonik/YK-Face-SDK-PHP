{
    "name": "youverse/face-api",
    "description": "Functionalities for biometric processing (detection, verification and identification) for Youverse.Face.",
    "keywords": [
        "Youverse",
        "Face Authentication",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://youverse.id/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "dev-yoonik",
            "homepage": "https://github.com/dev-yoonik"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": ">=6.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Youverse\\Face\\" : "src/" }
    }
}
